<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:c="http://www.springframework.org/schema/c"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


    <!-- 在spring配置文件头部添加 xmlns:c="http://www.springframework.org/schema/c"-->

    <bean id="people1" class="com.py.spring6.bean.People" c:name="Amber" c:sex="少女" c:birth-ref="birth2" />
    <bean id="people2" class="com.py.spring6.bean.People" c:_0="Nahida" c:_1="萝莉" c:_2-ref="birth2" />

    <bean id="birth2" class="java.util.Date"/>


</beans>